{
  "name": "hotel",
  "version": "0.5.4",
  "description": "Local domains for everyone and more! ",
  "main": "lib",
  "bin": "bin/index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "test": "ava && standard",
    "start": "npm run nodemon & npm run webpack -- --watch & wait",
    "prepublish": "npm run build",
    "uninstall": "node bin/uninstall.js",
    "precommit": "npm test",
    "nodemon": "nodemon -- src/daemon",
    "build": "npm run webpack && npm run babel",
    "babel": "rimraf lib && babel src -d lib --copy-files --ignore src/front",
    "webpack": "rimraf dist && webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typicode/hotel.git"
  },
  "keywords": [
    "dev",
    "utility",
    "process",
    "manager",
    "local",
    "server",
    "host",
    "proxy"
  ],
  "author": "Typicode <typicode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typicode/hotel/issues"
  },
  "homepage": "https://github.com/typicode/hotel",
  "dependencies": {
    "ansi2html": "0.0.1",
    "array-find": "^1.0.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.2.0",
    "connect-sse": "^1.2.0",
    "exit-hook": "^1.1.1",
    "express": "^4.13.3",
    "get-port": "^2.0.0",
    "http-proxy": "^1.12.0",
    "matcher": "^0.1.2",
    "mkdirp": "^0.5.1",
    "once": "^1.3.2",
    "os-homedir": "^1.0.1",
    "respawn": "^2.4.1",
    "server-ready": "^0.2.0",
    "strip-ansi": "^3.0.0",
    "sudo-block": "^1.2.0",
    "tildify": "^1.1.2",
    "unquote": "^1.1.0",
    "untildify": "^2.1.0",
    "update-notifier": "^1.0.0",
    "user-startup": "^0.2.1",
    "vhost": "^3.0.2",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.1.2",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.6.1",
    "escape-html": "^1.0.3",
    "husky": "^0.11.3",
    "lodash.difference": "^4.3.0",
    "mock-fs": "^3.9.0",
    "nodemon": "^1.8.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "standard": "^7.1.2",
    "supertest": "^1.2.0",
    "uid": "0.0.2",
    "vue": "^1.0.18",
    "webpack": "^1.12.13",
    "whatwg-fetch": "^1.0.0"
  },
  "ava": {
    "serial": true,
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
